{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "last 2 Chrome versions"
          ]
        }
      }
    ],
    "react",
    "flow"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "add-module-exports",
    "transform-async-to-generator",
    [
      "babel-plugin-styled-components",
      {
        "minify": false,
        "uglifyPure": false
      }
    ]
  ],
  "env": {
    "production:cjs": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "react",
        "flow"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-runtime"
      ],
      "ignore": [
        "__mocks__",
        "__fixtures__",
        "test",
        "coverage",
        "node_modules"
      ]
    },
    "production:esm": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ],
        "react",
        "flow"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-runtime"
      ],
      "ignore": [
        "__mocks__",
        "__fixtures__",
        "__tests__",
        "coverage",
        "node_modules"
      ]
    },
    "test": {
      "presets": [
        "env",
        "react",
        "flow"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}
